            <button id="btn-GR2Modificar5" label="Hierarquia de Produto" imageMso="ModuleInsert" onAction="MyRibbon.CallMM02Hierarquia" screentip="Alterar Hierarquia de Produto" />
            <button id="btn-GR2Modificar6" label="Cliente Responsável (Variante)" imageMso="AddOrRemoveAttendees" onAction="MyRibbon.CallMM02ClienteResponsavel" screentip="Alterar cliente responsavel" />
            <button id="btn-GR2Modificar12" label="Depósito de Produção" imageMso="RelationshipDesignAllRelationships" onAction="MyRibbon.CallMM02DEPPROD" screentip="Alterar depósito de produção interno e externo da MRP2" />
