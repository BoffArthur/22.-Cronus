            <button id="ExpandirCentro" label="Centro (PROF)" imageMso="RmsNavigationBarHome" onAction="MyRibbon.CallExpCentroPROF" screentip="Expandir centro de materiais PROF já criados" />
            <menuSeparator id="menu3-sp2" title="Sem fonte" />
            <button id="ExpandirCanalDist" label="Canais de Distribuição" imageMso="ArrowStyleGallery" onAction="MyRibbon.CallExpCanalDist" screentip="Expandir canal de distribuição de itens já criados" />
