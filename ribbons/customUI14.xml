          <separator id="spr-excel2" />
          <button id="btn-GR4Excel4" label="Estudos de Viabilidades" imageMso="ReviseContents" size="large" onAction="MyRibbon.CallOpenWBEstudoViabilidade" screentip="Criar novo estudo de viabilidade" />
        </group>
