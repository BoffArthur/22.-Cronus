        <group id="GroupSAPCriar1" label="SAP - Criar" >
          <menu id="menu-1" size="large" imageMso="SmartArtAddShapeSplitMenu" label="Novo" screentip="Criar um novo item" >
            <menuSeparator id="separator-modify" title="Atual como fonte" />
            <button id="btn-GR1Criar3" label="Prematerial" imageMso="SourceControlAddDatabase" onAction="MyRibbon.CallCriarMP" screentip="Criar novo prematerial (bobina+tira)" /> 
            <button id="btn-GR1Criar1" label="PROF" imageMso="_3DDirectionGalleryClassic" onAction="MyRibbon.CallCriarPROF" screentip="Criar novo material PROF (RollForming)" />
            <button id="btn-GR1Criar2" label="After Operation" imageMso="SmartArtAddShapeAbove" onAction="MyRibbon.CallCriarAO" screentip="Criar novo material A.O. (Agregados)" />  
            <button id="btn-GR1Criar5" label="PURLINS" imageMso="ComAddInsDialog" onAction="MyRibbon.CallCriarPURLINS" screentip="Criar novo item de PURLINS" />
            <menuSeparator id="separator-new" title="Sem fonte" />
            <button id="btn-GR1Criar4" label="LNTR" imageMso="AdpOutputOperationsAddToOutput" onAction="MyRibbon.CallCriarLNTR" screentip="Criar novo código Lenght Neutral" />
          </menu>
          <menu id="menu-3" size="large" imageMso="AdpDiagramAddRelatedTables" label="Expandir" screentip="Expandir um item existente" >
