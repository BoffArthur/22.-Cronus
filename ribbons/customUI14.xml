        <group id="GroupExcelWB" label="Pastas de Trabalho">
          <button id="btn-GR4Excel1" label="Estrutura de Cadastros" imageMso="XmlSchema" size="large" onAction="MyRibbon.CallOpenWBEstrutura" screentip="Criar uma nova estrutura de cadastros" />
          <button id="btn-GR4Excel2" label="Liberação de Cadastros" imageMso="SendStatusReport" size="large" onAction="MyRibbon.CallOpenWBEnviarLiberar" screentip="Abrir WB para enviar novos cadastros para serem liberados" />
          <separator id="spr-excel1" />
