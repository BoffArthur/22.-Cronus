            <button id="btn-CódigoSAP" label="Código SAP" imageMso="OutlineGroup" onAction="MyRibbon.btnInserirCODSAP" screentip="Inserir função" />
            <button id="btn-ExtCome" label="Extrair Comentário" imageMso="ExportTextFile" onAction="MyRibbon.btnInserirExt_Comment" screentip="Inserir função" />
          </menu>
          <button id="btn-InsertStyles" label="Estilos" imageMso="GroupPivotTableStyles" onAction="MyRibbon.btnFormatar" screentip="Inserir estilo de cores e formatação padrão nesta pasta de trabalho" />
