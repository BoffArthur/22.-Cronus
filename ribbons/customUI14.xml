            <button id="btn-GR2Modificar9" label="LNTR Z_LNP_TST_058X" imageMso="TabOrder" onAction="MyRibbon.CallCU60AdicionarMP" screentip="Adicionar novo LNTR na tabela de materiais componentes" />
            <button id="btn-GR2Modificar10" label="LNTR Z_LNP_ANL_TST_058X" imageMso="ObjectNudgeRight" onAction="MyRibbon.CallCU60AdicionarVP" screentip="Adicionar novo LNTR na tabela de versões de produção" />
          </menu>
        </group>
        <group id="GourpSAPRelatorio" label="SAP - Relatórios"> 
